import pandas as pd

# Sample DataFrame
df = pd.DataFrame({
    'As Of Date': [1, 2, 3],
    'Sample Column': [4, 5, 6],
    'Another Example Column': [7, 8, 9]
})

# Stylize the DataFrame
styled_df = df.style.set_table_styles([
    {'selector': 'thead th', 'props': [('background-color', 'darkblue'), ('color', 'white'), ('font-weight', 'bold')]}
]).set_properties(**{'border': '1px solid black', 'padding': '8px'})

# Display the styled DataFrame in the Jupyter Notebook
styled_df
