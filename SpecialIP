import pandas as pd

def clipboard_to_list() -> list:
    """
    Reads data from the clipboard (assumed to be Excel data), converts the first row of
    the DataFrame into a list of column names, and returns this list.
    
    Returns:
    - A list of column names from the clipboard data.
    """
    # Read the data from the clipboard into a DataFrame
    df = pd.read_clipboard()
    
    # Get the column names from the DataFrame and convert to a list
    column_names = df.columns.tolist()
    
    # Print the list in the desired format
    print(f"Column names as list: {column_names}")
    
    return column_names

# Example usage:
column_list = clipboard_to_list()
