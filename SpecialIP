import pandas as pd

# Assuming 'df' is your DataFrame and 'Gross Coupon' is the column you want to bucket
# Define custom boundaries for the buckets (adjust these based on your requirements)
bins = [df['Gross Coupon'].min(), 2, 4, 6, 8, df['Gross Coupon'].max()]  # Custom bin edges
labels = ['Bucket 1', 'Bucket 2', 'Bucket 3', 'Bucket 4', 'Bucket 5']     # Custom labels

# Create the custom buckets using pd.cut
df['GrossCoupon_Bucket'] = pd.cut(df['Gross Coupon'], bins=bins, labels=labels, include_lowest=True)

# Display the first few rows to verify
print(df[['Gross Coupon', 'GrossCoupon_Bucket']])
