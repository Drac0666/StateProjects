import pandas as pd

# Sample DataFrame
data = {
    "Final Asset Class": ["Class A", "Class A", "Class B", "Class B", "Class B", "Class C"],
    "cusip": ["CUSIP1", "CUSIP2", "CUSIP3", "CUSIP4", "CUSIP5", "CUSIP6"]
}

df = pd.DataFrame(data)

# Group by Final Asset Class and add a new column with sequential numbers for each cusip
df['Cusip_Sequence'] = df.groupby('Final Asset Class').cumcount() + 1

print(df)
