import statsmodels.api as sm

# Prepare the data
X = df_filtered[['Amortisation Value']]
X = sm.add_constant(X)  # Adding a constant term for the intercept
y = df_filtered['DQ']

# Fit logistic regression model
model = sm.Logit(y, X)
result = model.fit()

# Print the summary of the logistic regression
print(result.summary())
