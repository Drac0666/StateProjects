import pandas as pd

# Assuming your DataFrame is named df

# Define your filters
asset_class_filter = df['Asset Class'] == 'ABS'
sector1_filter = df['SECTOR1'].isin(['ABS', 'FRMBS'])
sector2_filter = df['SECTOR2'].isin(['Australia', 'Britain', 'CNSM', 'Italy', 'Netherlands'])

# Apply the filters
filtered_df = df[asset_class_filter & sector1_filter & sector2_filter]

# Display the filtered DataFrame
print(filtered_df)
