import pandas as pd

def drop_duplicated_indexes(df):
    """
    Drop duplicated indexes from a DataFrame.

    Parameters:
    df (pandas.DataFrame): The DataFrame from which to drop duplicated indexes.

    Returns:
    pandas.DataFrame: The DataFrame with duplicated indexes dropped.
    """
    df_no_duplicates = df[~df.index.duplicated(keep='first')]
    print("Duplicated indexes dropped from the DataFrame.")
    return df_no_duplicates

# Example usage
if __name__ == "__main__":
    # Creating a sample DataFrame with duplicated indexes
    data = {
        'Date': ['2024-01-01', '2024-01-02', '2024-01-03', '2024-01-04'],
        'FX': [1.1, 1.2, 1.3, 1.4]
    }
    df = pd.DataFrame(data, index=[0, 1, 1, 2])
    print("Original DataFrame with duplicated indexes:")
    print(df)
    
    # Dropping duplicated indexes
    df_no_duplicates = drop_duplicated_indexes(df)
    print("DataFrame after dropping duplicated indexes:")
    print(df_no_duplicates)
