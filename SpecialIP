INT_renamed = INT.rename(
    columns=lambda c: f"INT_{c}" if c != 'Deal' else c
)

rows_before = len(DF)

DF_merged = DF.merge(
    INT_renamed,
    left_on='Security',
    right_on='Deal',
    how='left'
)

rows_after = len(DF_merged)

if rows_before != rows_after:
    print(f"⚠️ Row count changed! Before: {rows_before}, After: {rows_after}")
else:
    print(f"✅ Row count OK: {rows_before}")

