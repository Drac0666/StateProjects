import pandas as pd

def infer_date_format_trial(date_str):
  formats = ['%Y-%m-%d', '%m/%d/%Y', '%d/%m/%Y']  # Add more formats
  for fmt in formats:
    try:
      pd.to_datetime(date_str, format=fmt)
      return fmt
    except ValueError:
      pass
  return None

# Example usage
data = {'date': ['2023-11-24', '03/12/2023', '15-01-2024']}
df = pd.DataFrame(data)

df['format'] = df['date'].apply(infer_date_format_trial)
print(df)
