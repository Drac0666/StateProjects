df_list = [
    ('df_2020_a', pd.DataFrame({'Jan': [2020, 1000], 'Feb': [2020, 2000], 'Mar': [2020, 1500]}, index=['Vintage', 'Current Balance'])),
    ('df_2020_b', pd.DataFrame({'Jan': [2020, 500], 'Feb': [2020, 800], 'Mar': [2020, 700], 'Apr': [2020, 900]}, index=['Vintage', 'Current Balance']))
]
all_columns = set(col for _, df in df_list for col in df.columns)
