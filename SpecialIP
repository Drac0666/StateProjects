import pandas as pd
import statsmodels.api as sm

def logistic_regression_analysis(df, columns_to_check):
    """
    Fits a logistic regression model to predict DQ using specified columns.
    
    Parameters:
    df (pd.DataFrame): The input dataframe with a 'DQ' column.
    columns_to_check (list): List of column names to use as predictors.
    
    Returns:
    pd.DataFrame: A summary of the logistic regression model.
    """
    # Select the columns to check as predictors and add a constant term for the intercept
    X = df[columns_to_check]
    X = sm.add_constant(X)
    
    # Define the target variable
    y = df['DQ']
    
    # Fit the logistic regression model
    model = sm.Logit(y, X).fit()
    
    # Return the summary of the model
    return model.summary()

# Example usage
# Assuming df is your dataframe and you want to check columns 'column1', 'column2', etc.
columns_to_analyze = ['column1', 'column2', 'column3']  # Replace with actual column names
logistic_model_summary = logistic_regression_analysis(df, columns_to_analyze)

# Display results
print(logistic_model_summary)
