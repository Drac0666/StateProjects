# zakładam że Order Date jest już w formacie datetime
# jeśli nie — dodaj konwersję:
# df["Order Date"] = pd.to_datetime(df["Order Date"])

# Tworzymy nową kolumnę w formacie DD/MM/YYYY - DD/MM/YYYY
df["DateRange"] = df["Order Date"].dt.strftime("%d/%m/%Y")
df["DateRange"] = df["DateRange"] + " - " + df["DateRange"]
