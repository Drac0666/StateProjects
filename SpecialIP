Sub FindFormulaResult()
    Dim ws As Worksheet
    Dim searchValue As Variant
    Dim foundCell As Range
    Dim rowNum As Long
    
    ' Set the worksheet (change "Sheet1" to your sheet name)
    Set ws = ThisWorkbook.Sheets("Sheet1")
    
    ' Set the value to search for (a date in this case)
    searchValue = DateValue("9/19/2024") ' Convert the date string to a date type
    
    ' Search for the value in Column A, including formula results
    Set foundCell = ws.Columns("A").Find(What:=searchValue, LookIn:=xlFormulas, LookAt:=xlWhole)
    
    ' Check if the value is found
    If Not foundCell Is Nothing Then
        ' Get the row number
        rowNum = foundCell.Row
        MsgBox "Value '" & searchValue & "' found in row: " & rowNum
    Else
        MsgBox "Value '" & searchValue & "' not found in Column A."
    End If
End Sub
