import pandas as pd
import pyarrow.feather as feather

# Example dictionary of DataFrames
dataframes = {
    "df1": pd.DataFrame({"a": [1, 2, 3], "b": [4, 5, 6]}),
    "df2": pd.DataFrame({"x": [7, 8, 9], "y": [10, 11, 12]})
}

# Save each DataFrame as a separate Feather file
for name, df in dataframes.items():
    feather.write_feather(df, f"{name}.feather")

# Load each DataFrame from a Feather file
loaded_dataframes = {name: feather.read_feather(f"{name}.feather") for name in dataframes.keys()}

# Display loaded DataFrames to verify
for name, df in loaded_dataframes.items():
    print(f"{name}:\n{df}\n")
