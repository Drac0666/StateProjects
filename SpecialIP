import pandas as pd
import scipy.stats as stats

# Sample DataFrame with 'Balance Categories' as string and 'DQ'
data = {'Balance Categories': ['Low', 'Medium', 'High', 'Low', 'Medium', 'High', 'Medium', 'Low', 'High', 'Medium'],
        'DQ': [1, 1, 0, 1, 0, 1, 1, 0, 1, 0]}
df = pd.DataFrame(data)

# Perform Chi-Square test to determine if there is a relationship between Balance Categories and DQ
contingency_table = pd.crosstab(df['Balance Categories'], df['DQ'])
chi2, p, dof, expected = stats.chi2_contingency(contingency_table)

# Results
print("Chi-Square Test Results")
print(f"Chi-Square Statistic: {chi2}")
print(f"P-value: {p}")
if p < 0.05:
    print("There is a significant association between Balance Categories and DQ.")
else:
    print("There is no significant association between Balance Categories and DQ.")
