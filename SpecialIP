# Define a dictionary to map quantile values to custom labels
quantile_labels = {
    0.001: 'Left 0.1%',
    0.005: 'Left 0.5%',
    0.010: 'Left 1%',
    0.950: 'Right 5%',
    0.990: 'Right 1%',
    0.995: 'Right 0.5%',
    0.999: 'Right 0.1%',
    0.9999: 'Right 0.01%'
}

# Apply the custom labels to the 'Quantile' index
pivot_df = pivot_df.rename(index=quantile_labels)

# Display the relabeled DataFrame
pivot_df
