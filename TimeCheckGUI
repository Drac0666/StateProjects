import csv

def read_csv_to_list(csv_file):
    data = []
    with open(csv_file, 'r', newline='') as file:
        csv_reader = csv.reader(file)
        for row in csv_reader:
            data.append(row)
    return data

# Example usage:
csv_file_path = 'example.csv'  # Replace with the path to your CSV file
csv_data = read_csv_to_list(csv_file_path)

for row in csv_data:
    print(row)
