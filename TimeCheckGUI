import re

def extract_hh_mm(file_name):
    # Define a regex pattern to match "<HH>_<MM>" in the file name
    pattern = r'_(\d+)_(\d+)_'

    # Use re.search to find the HH and MM values in the file name
    match = re.search(pattern, file_name)

    if match:
        hh = match.group(1)
        mm = match.group(2).zfill(2)
        hh_mm = f"{hh}.{mm}"
        return hh_mm
    else:
        return None

# Example usage
file_name = "SomeRandomText_9_5_restoffile"
hh_mm = extract_hh_mm(file_name)

if hh_mm:
    print(f"Extracted HH.MM: {hh_mm}")
else:
    print("Pattern not found in the file name.")
