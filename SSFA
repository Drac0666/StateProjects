import pandas as pd

def dataframes_to_excel(dataframes, excel_file):
    """
    Save a list of DataFrames to an Excel file with each sheet named after the DataFrame variable name.

    Parameters:
    dataframes (list): List of DataFrames to be written to the Excel file.
    excel_file (str): The path of the Excel file to save the DataFrames.
    """
    with pd.ExcelWriter(excel_file, engine='xlsxwriter') as writer:
        for df in dataframes:
            # Find the variable name of the DataFrame
            df_name = [name for name in globals() if globals()[name] is df][0]
            # Write the DataFrame to the specified sheet
            df.to_excel(writer, sheet_name=df_name, index=False)

# Sample DataFrames
df1 = pd.DataFrame({
    'Asset Class': ['A', 'B', 'C'],
    'EAD': [100, 200, 300],
    'RW': [0.5, 0.7, 0.9]
})

df2 = pd.DataFrame({
    'Category': ['X', 'Y', 'Z'],
    'Value': [10, 20, 30]
})

# List of DataFrames
dataframes = [df1, df2]

# Save to Excel file
dataframes_to_excel(dataframes, 'output.xlsx')
