import customtkinter as ctk
import psutil

ctk.set_appearance_mode("dark")  
ctk.set_default_color_theme("blue")  

def kill_process(process_name):
    for proc in psutil.process_iter():
        if process_name.lower() in proc.name().lower():  # "Like" search
            proc.kill()

root = ctk.CTk()
root.title("Process Killer")
root.geometry("300x200") 

btn_intex = ctk.CTkButton(root, text="Kill Intex", command=lambda: kill_process("intex"))
btn_powerpoint = ctk.CTkButton(root, text="Kill PowerPoint", command=lambda: kill_process("powerpnt"))
btn_excel = ctk.CTkButton(root, text="Kill Excel", command=lambda: kill_process("excel"))
btn_word = ctk.CTkButton(root, text="Kill Word", command=lambda: kill_process("winword"))

btn_intex.pack(pady=10) 
btn_powerpoint.pack(pady=10)
btn_excel.pack(pady=10)
btn_word.pack(pady=10)

root.mainloop()
